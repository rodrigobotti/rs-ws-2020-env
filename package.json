{
  "name": "rs-ws-env",
  "version": "1.0.0",
  "description": "",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "start:dev": "NODE_ENV=dev nodemon --inspect server/index.js",
    "test": "npm run test:unit && npm run test:integration",
    "test:cover": "nyc npm test",
    "test:unit": "NODE_ENV=test mocha --require tests/helpers \"api/**/*@(.spec.js)\" --timeout 5000",
    "test:unit:debug": "NODE_ENV=test mocha --inspect-brk --require tests/helpers \"api/**/*@(.spec.js)\" --timeout 5000",
    "test:integration": "NODE_ENV=test mocha --require tests/helpers \"tests/api/**/*@(.spec.js)\" --timeout 5000",
    "modulealias:link": "link-module-alias",
    "modulealias:clean": "command -v link-module-alias && link-module-alias clean || true",
    "postinstall": "npm run modulealias:link",
    "preinstall": "npm run modulealias:clean"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "dotenv": "^8.2.0",
    "eslint": "^7.13.0",
    "eslint-config-standard": "^16.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-json": "^2.1.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.1.0",
    "eslint-watch": "^7.0.0",
    "faker": "^5.1.0",
    "link-module-alias": "^1.2.0",
    "mocha": "^8.2.1",
    "nodemon": "^2.0.6",
    "nyc": "^15.1.0",
    "sinon": "^9.2.1",
    "sinon-chai": "^3.5.0",
    "supertest": "^6.0.1"
  },
  "dependencies": {
    "koa": "^2.13.0",
    "koa-async-validator": "^0.4.1",
    "koa-bodyparser": "^4.3.0",
    "koa-router": "^10.0.0",
    "moment": "^2.29.1",
    "mongodb": "^3.6.3",
    "pino": "^6.2.0",
    "pino-pretty": "^4.0.0",
    "ramda": "^0.27.1",
    "uuid": "^8.3.1"
  },
  "_moduleAliases": {
    "_application": "api/application",
    "_domain": "api/domain",
    "_usecases": "api/usecases",
    "_adapters": "api/adapters",
    "_api": "api",
    "_config": "config",
    "_infrastructure": "infrastructure",
    "_server": "server",
    "_tests": "tests"
  }
}
